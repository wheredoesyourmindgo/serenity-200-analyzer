{
    "desc": "Usage: ./a200 [command] [args]",
    "actions": [
        {
            "name": "view",
            "alias": "vw",
            "args": ["layouts(s)"],
            "desc": "get detailed data on a particular layout"
        },
        {
            "name": "toggle column",
            "alias": "tc",
            "args": ["column(s)", "all"],
            "desc": "change the visibility of some column(s)"
        },
        {
            "name": "toggle layout",
            "alias": "tl",
            "args": ["layouts(s)", "all"],
            "desc": "change the visibility of some layout(s)"
        },
        {
            "name": "sort",
            "alias": "st",
            "args": ["parameter(s)"],
            "desc": "order the results based on the parameters"
        },
        {
            "name": "filter",
            "alias": "fl",
            "args": ["parameters(s)"],
            "desc": "filter the results based in the parameters"
        },
        {
            "name": "thumb",
            "alias": "tb",
            "args": ["LT", "RT", "NONE", "AVG"],
            "desc": "set which thumb presses space"
        },
        {
            "name": "data",
            "alias": "dt",
            "args": ["dataname"],
            "desc": "set the data to use for the analysis"
        },
        {
            "name": "theme",
            "alias": "tm",
            "args": ["themename"],
            "desc": "set the color scheme"
        },
        {
            "name": "config save",
            "alias": "cs",
            "args": ["name"],
            "desc": "save the current config"
        },
        {
            "name": "config load",
            "alias": "cl",
            "args": ["configfile"],
            "desc": "load a config from file"
        },
        {
            "name": "cache clear",
            "alias": "cc",
            "args": [],
            "desc": "clear the cache"
        },
        {
            "name": "reset",
            "alias": "",
            "args": [],
            "desc": "reset the config to its factory defaults"
        }
    ]
}